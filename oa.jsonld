        },
        "rdfs:subClassOf": "schema:LocationFeatureSpecification"
      },
      {
        "@id": "oa:Creche",
        "@type": "rdfs:Class",
        "rdfs:label": {
          "en": "Creche"
        },
        "rdfs:comment": {
          "en": "Creche"
        },
        "rdfs:subClassOf": "schema:LocationFeatureSpecification"
      },
      {
        "@id": "oa:Parking",
        "@type": "rdfs:Class",
        "rdfs:label": {
          "en": "Parking"
        },
        "rdfs:comment": {
          "en": "Parking"
        },
        "rdfs:subClassOf": "schema:LocationFeatureSpecification"
      },
      {
        "@id": "oa:BabyChanging",
        "@type": "rdfs:Class",
        "rdfs:label": {
