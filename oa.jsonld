        },
        "rdfs:subClassOf": "oa:GenderRestrictionType"
      },
      {
        "@id": "oa:None",
        "@type": "rdfs:Class",
        "rdfs:label": {
          "en": "None"
        },
        "rdfs:comment": {
          "en": "Enumerated value for specifying gender restrictions"
        },
        "rdfs:subClassOf": "oa:GenderRestrictionType"
      },
      {
        "@id": "oa:FacilityUse",
        "@type": "rdfs:Class",
        "rdfs:label": {
          "en": "FacilityUse"
        },
        "rdfs:comment": {
          "en": "An opportunity to use a facility"
        },
        "rdfs:subClassOf": "schema:Product"
      },
      {
        "@id": "oa:ChangingRooms",
        "@type": "rdfs:Class",
        "rdfs:label": {
